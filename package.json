{
  "name": "telkomdev-react-native-codebase",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "eslint": "eslint src index.* --ext .js --cache",
    "eslint:fix": "npm run eslint -- --fix",
    "start": "react-native start",
    "test": "jest",
    "test:cover": "jest --coverage",
    "lint": "eslint .",
    "postinstall": "node ./node_modules/husky/lib/installer/bin install"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/netinfo": "^5.3.3",
    "axios": "^0.19.0",
    "js-file-download": "^0.4.9",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "querystring": "^0.2.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-device-info": "^5.5.1",
    "react-native-elements": "^1.2.7",
    "react-native-fs": "^2.16.2",
    "react-native-modal": "^11.5.3",
    "react-native-navigation": "^4.5.1",
    "react-native-svg": "^11.0.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^8.0.6",
    "react-native-zip-archive": "^5.0.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-form": "^8.2.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/runtime": "7.8.3",
    "@react-native-community/eslint-config": "0.0.5",
    "babel-jest": "24.9.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-native": "^3.8.1",
    "husky": "^4.2.5",
    "jest": "24.9.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "metro-react-native-babel-preset": "0.56.4",
    "pre-commit": "^1.2.2",
    "react-dom": "^16.12.0",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "16.9.0",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "setupFiles": [
      "./enzymeTestAdapterSetup.js",
      "./jestSetupFile.js",
      "./__mocks__/deviceInfoMock.js",
      "./__mocks__/reactNativeFs.js",
      "./__mocks__/axios.js"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.js"
    },
    "collectCoverageFrom": [
      "src/components/**/*.js",
      "src/screens/**/*.js",
      "!src/screens/ExamplePage/**/*.js",
      "!src/screens/Initializer.js",
      "!src/screens/Tabs.js",
      "src/utils/**/*.js"
    ],
    "testURL": "http://localhost/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint",
      "pre-push": "npm run eslint"
    }
  }
}
